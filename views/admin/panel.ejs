<%- include ../templates/header -%>
    <center><h1>Administrador</h1><center>
    
<div class="container">
    <br><br><br>
    <div class="row">
        <div class="col-md-12">
            <div class="well well-sm">
                <form  action="/admin/panel" class="form-horizontal" method="POST">
                    <fieldset>
                        <legend class="text-center header"> <IMG SRC="..\public\img\SIADCON.png" WIDTH=220 HEIGHT=160>Pago de Servicios</legend>
                        <div class="form-group">
                            <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-home bigicon"></i></span>
                            <div class="col-md-8">
                                <input required id="dep" name="dep" type="text" placeholder="Núm. de departamento" class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-cogs bigicon"></i></span>
                            <div class="col-md-8">
                                <input required id="limpieza" name="limpieza" type="number" placeholder="Limpieza" class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-bolt bigicon"></i></span>
                            <div class="col-md-8">
                                <input required id="energia" name="energia" type="number" placeholder="Energia Electrica Areas Comunes" class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-lock bigicon"></i></span>
                            <div class="col-md-8">
                                <input required id="seguridad" name="seguridad" type="number" placeholder="Seguridad" class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-trash-o"></i></span>
                            <div class="col-md-8">
                                <input id="basura" name="basura" type="number" placeholder="Basura" class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-phone bigicon"></i></span>
                            <div class="col-md-8">
                                <input id="telefono" name="telefono" type="number" placeholder="Telefono" class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-leaf bigicon"></i></span>
                            <div class="col-md-8">
                                <input id="jardineria" name="jardineria" type="number" placeholder="Jardinería y fumigaciones" class="form-control">
                            </div>
                        </div>
                         <div class="form-group">
                            <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-fire bigicon"></i></span>
                            <div class="col-md-8">
                                <input id="gas" name="gas" type="number" placeholder="Suministro de gas" class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-tint bigicon"></i></span>
                            <div class="col-md-8">
                                <input id="predial" name="predial" type="number" placeholder="Predial" class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-male bigicon"></i></span>
                            <div class="col-md-8">
                                <input id="honorarios" name="honorarios" type="number" placeholder="Honorarios del Admon" class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-plus-square bigicon"></i></span>
                            <div class="col-md-8">
                                <input id="otros" name="otros" type="number" placeholder="Otros Servicios (plomeria, impermeabilizante, albañileria, etc.)" class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-money bigicon"></i></span>
                            <div class="col-md-8">
                                <input id="abonar" name="abonar" type="number" placeholder="Pago del condomino" class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-12 text-center">
                                <button type="submit" class="btn btn-success btn-lg" value="ENVIAR">Cobrar</button>
                            </div>
                        </div>
                    </fieldset>
                </form>
            </div>
        </div>
    </div>
</div>
<%- include ../templates/footer -%>